<script type="text/javascript">
        $(document).ready(function () {
			LoadAnimations();
        });
		function LoadAnimations() {
			LoadImage('/Media/Default/animations/CROC-IN-CUP-NEW.gif', ShowImageFunc('#imgAboutUs', 'imgAboutUs', 400));
			LoadImage('/Media/Default/animations/REVIEWS.gif', ShowImageFunc('#imgReviews', 'imgReviews', 300));
			LoadImage('/Media/Default/animations/LEGS.gif', ShowImageFunc('#imgGigs', 'imgGigs', 200));
		    if($(window).width() > 960) {	
				LoadImage('/Media/Default/animations/CHIC-SHOE.gif', ShowImageFunc('#imgNews', 'imgNews', 300));
				LoadImage('/Media/Default/animations/GALLERY.gif', ShowImageFunc('#imgGallery', 'imgGallery', 500));
				LoadImage('/Media/Default/animations/EDIN.gif', ShowImageFunc('#imgEdinburgh', 'imgEdinburgh', 600));
				LoadImage('/Media/Default/animations/FISH-AND-NAIL.gif', ShowImageFunc('#imgContact', 'imgContact', 700));
				LoadImage('/Media/Default/animations/FACEBOOK.gif', ShowImageFunc('#imgFacebook', 'imgFacebook', 400));
			}		
		}
        function LoadImage(url, callback, delay) {
            var img = new Image();
            img.onload = callback;
            img.src = url;
        }
        function ShowImageFunc(selector, cssClass, delay)
        {
            return function () {
				if(!delay)
					delay = 2000;
                $(selector+'').hide().delay(delay).addClass(cssClass).fadeIn('slow');
            }
        }
</script>


